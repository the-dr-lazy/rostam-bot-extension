{
  "manifest_version": 2,
  "name": "Rostam Bot",
  "description": "PLACE_HOLDER",
  "version": "PLACE_HOLDER",
  "permissions": ["https://*/*", "http://*/*"],
  "content_scripts": [
    {
      "matches": ["http://twitter.com/*", "https://twitter.com/*"],
      "js": ["content-script.js"],
      "css": ["content-script.css"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "web_accessible_resources": ["assets/**/*"]
}
